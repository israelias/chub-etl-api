{
  "name": "chub-etl-api",
  "version": "1.0.0",
  "description": "Thrift DJ-DRF Models and Serverless ETL Functions using Vercel's Python Runtime",
  "author": "israelias <israelias.js@gmail.com>",
  "repository": "israelias/chub-etl-api",
  "license": "MIT",
  "scripts": {
    "virtualenv": "pip install virtualenv && virtualenv venv",
